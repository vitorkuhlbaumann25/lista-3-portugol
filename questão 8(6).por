programa {
  funcao inicio() {
    inteiro numero
    escreva("informe um numero: ")
    leia(numero)
    se(numero%3==0){
      escreva("este numero e multiplo de tres. ")
    }
    se(numero%5==0){
      escreva("\neste numero e multiplo de cinco. ")
    }se(numero%3!=0 e numero%5!=0)
    {
      escreva("este numero nao e multiplo de 3.\n")
    }
  }
}
